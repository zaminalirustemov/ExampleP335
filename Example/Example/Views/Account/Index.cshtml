@model RegisterViewModel

<form asp-controller="account" asp-action="index">
    <div class="form-group">
        <label asp-for="Username">Username</label>
        <input asp-for="Username" class="form-control" placeholder="Enter Username">
    </div>
    <div class="form-group">
        <label asp-for="Phone">PhoneNumber</label>
        <input asp-for="Phone" class="form-control" placeholder="Enter PhoneNumber">
    </div>
    <div class="form-group">
        <label asp-for="Pass">Password</label>
        <input asp-for="Pass"  class="form-control" placeholder="Enter Password">
    </div>
    <div class="form-group">
        <label asp-for="ConfirPass">Confirm Password</label>
        <input asp-for ="ConfirPass" class="form-control" placeholder="Enter Confirm Password">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>